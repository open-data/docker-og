#
# CKAN Registry
# registry.open.local
#
server {

    server_name registry.open.local;

    listen 80;

    include conf/default_settings.conf;
    include conf/header_settings.conf;

    set $upstreamHost "backend_host_ckan";
    set $upstremProtocol "http://";

    location / {

        include conf/proxy_settings.conf;

    }

}
# END
# CKAN Registry
# END